int white_checkers(void){
  printf("%c%c%c\n%c%c%c\n",218,196,191,192,196,217);
}

int black_checkers(void){
  printf("\n%c%c%c",222,219,221);
}

int new_game( void ){
	
	int random, white, black;
	

	srand ( time ( NULL ) );
	random = rand() % 100 + 1;

	if( random >= 50 ){
		printf( "White Pieces begin first\n" );
		}
	else if( random < 50 ){
		printf( "Black Pieces begin first\n" );
		}
}

int menu( void ){

	int option;

	 printf( "\nMenu:\n1 - New Game\n0 - Exit\n"); 
	 printf( "\nOption: " ); 
	 scanf( "%d", &option );

switch ( option ){

		case 1: new_game();
				break; 
	}
	return option;
}

int main( void ){

	int option = 3;

	while( option > 0 ) { 
		 option = menu();
		}

	printf( "\nEnd of program.\n" ); 
	return 0;
}
